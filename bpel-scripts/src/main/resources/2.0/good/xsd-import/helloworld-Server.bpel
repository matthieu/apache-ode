    <bpel:assign bpmn:label="invoke" bpmn:id="_FrsFcEpAEdySiIWBetfiFA">
      <bpel:copy>
        <bpel:from>"test"</bpel:from>
        <bpel:to>$Data/tns:Name</bpel:to>
